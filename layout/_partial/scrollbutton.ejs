<% if (!theme.scrollUpAnimation) { %>
  <a 
    role="button" 
    id="scrollbutton" 
    class="basebutton" 
    href="#"
    aria-label="回到顶部">
    <i class="fa-solid fa-angle-left"></i>
  </a>
<% } else { %>
  <a 
    role="button" 
    id="scrollbutton" 
    class="basebutton" 
    aria-label="回到顶部">
    <i class="fa-solid fa-angle-left"></i>
  </a>
<% } %>

<% if (!config.toc || config.toc.showListNumber !== false) { %>
<a 
  role="button" 
  id="menubutton"
  aria-label="menu button"
  class="basebutton">
  <i class="fa-solid fa-bars"></i>
</a>
<% } %>

<a 
  role="button" 
  id="popbutton" 
  class="basebutton" 
  aria-label="控制中心">
  <i class="fa-solid fa-list"></i>
</a>

<a 
  role="button" 
  id="darkbutton" 
  class="basebutton darkwidget" 
  aria-label="夜色模式">
  <i class="fa-solid fa-moon"></i>
</a>
<% if (theme.search && theme.search.enable) { %> <!-- ここでチェック -->
<a 
  role="button" 
  id="searchbutton" 
  class="basebutton searchwidget" 
  aria-label="搜索">
  <i class="fa-solid fa-magnifying-glass"></i>
</a>
<% } %> <!-- チェックの終わり -->
